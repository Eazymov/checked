{
  "name": "checked",
  "version": "0.0.0",
  "description": "Check type of value with flow and typescript support",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "unpkg": "dist/index.min.js",
  "jsdelivr": "dist/index.min.js",
  "files": [
    "dist/*",
    "cjs/*",
    "es/*",
    "LICENSE",
    "README.md"
  ],
  "repository": "git@github.com:Eazymov/checked.git",
  "keywords": [
    "javascript",
    "type",
    "check",
    "flow",
    "typescript"
  ],
  "author": {
    "name": "Eduard Troshin",
    "email": "eazymovcode@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/Eazymov/checked/issues"
  },
  "homepage": "https://github.com/Eazymov/checked#readme",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "babel-eslint": "^10.0.1",
    "copyfiles": "^2.1.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.14.0",
    "eslint-plugin-flowtype": "^3.4.2",
    "flow-bin": "^0.93.0",
    "flow-coverage-report": "^0.6.1",
    "flow-typed": "^2.5.1",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4",
    "rollup": "^1.2.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-uglify": "^6.0.2",
    "tiny-invariant": "^1.0.3",
    "typescript": "^3.3.3",
    "yargs": "^13.2.0"
  },
  "scripts": {
    "build": "yarn build:cjs && yarn build:es && yarn build:iife",
    "build:cjs": "node build.js --mode=cjs",
    "build:es": "node build.js --mode=es",
    "build:iife": "cross-env BABEL_ENV=iife rollup -c",
    "build:ts": "copyup \"src/**/*.d.ts\"",
    "prettier": "prettier \"./**/*.{js,flow,json,md}\"",
    "prettier:check": "yarn prettier --check",
    "prettier:write": "yarn prettier --write",
    "flow:restart": "flow stop && flow start",
    "flow:coverage": "flow-coverage-report --config flow-coverage.json",
    "lint": "yarn lint:js",
    "lint:js": "eslint \"src/**/*.js\"",
    "lint:staged": "lint-staged",
    "test": "yarn test:unit && yarn test:types",
    "test:unit": "BABEL_ENV=test jest",
    "test:watch": "yarn test:unit --watch",
    "test:coverage": "BABEL_ENV=test jest --coverage",
    "test:types": "yarn test:types:ts && yarn test:types:flow",
    "test:types:ts": "tsc -p tsconfig.json",
    "test:types:flow": "flow check",
    "coveralls": "yarn test:coverage --coverageReporters=text-lcov | coveralls"
  }
}
